{
    "name": "codeonyii/yii2-at-least-validator",
    "description": "Validates at least one (or more) attributes.",
    "type": "yii2-extension",
    "keywords": ["yii2","validator","at least","validation"],
    "homepage": "https://github.com/code-on-yii/yii2-at-least-validator",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sidney Lins",
            "email": "slinstj@gmail.com"
        }
    ],
    "replace": {
        "bower-asset/jquery": ">=1.11.0",
        "bower-asset/inputmask": ">=3.2.0",
        "bower-asset/punycode": ">=1.3.0",
        "bower-asset/yii2-pjax": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "codeonyii\\yii2validators\\": ""
        }
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^7.1",
        "yiisoft/yii2": "~2.0.5"
      },
    "require-dev": {
        "phpunit/phpunit": "6.5.8"
    }
}
